  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build && node scripts/copy-pwa-files.js",
    "build:railway": "vite build",
    "db:push:conditional": "if [ \"$CONTEXT\" = \"production\" ]; then npm run db:push; else echo 'Skipping database migration - not in production context'; fi",
    "start": "NODE_ENV=production tsx server/index.ts",
    "production:seed": "tsx server/production-seed.ts",
    "check": "tsc",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run tests/unit",
    "test:integration": "jest --config=jest.config.js",
    "test:all": "npm run test:run && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:integration && npm run test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:invitations": "playwright test --config=playwright-invitations.config.ts",
    "test:e2e:invitations:ui": "playwright test --config=playwright-invitations.config.ts --ui",
    "test:e2e:invitations:debug": "playwright test --config=playwright-invitations.config.ts --debug",
    "test:e2e:invitations:headed": "playwright test --config=playwright-invitations.config.ts --headed",
    "coverage:report": "vitest run --coverage && open coverage/index.html",
    "coverage:ci": "vitest run --coverage --reporter=lcov",
    "performance:test": "lhci autorun",
    "db:push": "drizzle-kit push",
    "db:seed": "tsx server/seed.ts",
    "db:production-seed": "tsx server/production-seed.ts",
    "db:add-test-cruises": "tsx server/add-test-cruises.ts",
    "db:seed:local": "DATABASE_URL=file:./cruise-guide.db tsx server/seed.ts",
    "deploy:production": "node deploy-to-production.js",
    "api:generate-types": "node scripts/generate-api-types.js",
    "api:docs": "echo 'API Documentation available at http://localhost:3001/api/docs'"
  },
